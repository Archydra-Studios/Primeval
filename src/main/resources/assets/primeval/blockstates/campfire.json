{
  "multipart": [
    {
      "when": {
        "kindling": 0
      },
      "apply": {
        "model": "primeval:block/campfire_0"
      }
    },
    {
      "when": {
        "kindling": 1
      },
      "apply": {
        "model": "primeval:block/campfire_1"
      }
    },
    {
      "when": {
        "kindling": 2
      },
      "apply": {
        "model": "primeval:block/campfire_2"
      }
    },
    {
      "when": {
        "kindling": 3
      },
      "apply": {
        "model": "primeval:block/campfire_3"
      }
    },

    {
      "when": {
        "fire_scale": 0,
        "lit": "true"
      },
      "apply": {
        "model": "primeval:block/campfire_fire_0"
      }
    },
    {
      "when": {
        "fire_scale": 1,
        "lit": "true"
      },
      "apply": {
        "model": "primeval:block/campfire_fire_1"
      }
    },
    {
      "when": {
        "fire_scale": 2,
        "lit": "true"
      },
      "apply": {
        "model": "primeval:block/campfire_fire_2"
      }
    },
    {
      "when": {
        "fire_scale": 3,
        "lit": "true"
      },
      "apply": {
        "model": "primeval:block/campfire_fire_3"
      }
    }
    
  ]
}